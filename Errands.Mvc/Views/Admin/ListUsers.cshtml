@model IEnumerable<User>

@foreach (var user in Model)
{
<p>@user.FirstName</p>
<p>@user.LastName</p>
<p>@user.UserName</p>
<p>@user.Email</p>
<form asp-action="DeleteAndBlockedUser" method="post">
    <input type="hidden" name="userId" value="@user.Id" />
    <button type="submit">Delete and blocked user</button>
</form>
}
<a asp-controller="Admin" asp-action="ListBlockedEmails">ListBlockedUserNames</a>